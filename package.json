{
  "name": "link-redux",
  "version": "1.0.0-beta32",
  "description": "React/Redux extensions for the Link library",
  "main": "dist/link-redux.umd.js",
  "module": "dist/link-redux.es6.js",
  "sideEffects": false,
  "typings": "dist/typings/link-redux.d.ts",
  "files": [
    "@types",
    "dist/link-redux.es6.js",
    "dist/link-redux.umd.js",
    "dist/typings/**/*.*",
    "package.json",
    "tsconfig.json",
    "yarn.lock"
  ],
  "repository": "https://github.com/fletcher91/link-redux.git",
  "scripts": {
    "build": "rollup -c && tsc -p tsconfig-types.json",
    "doc": "rm -rf dist/docs/* && typedoc --tsconfig tsconfig-types.json --exclude **/*.spec.ts --excludeExternals --excludePrivate --mode file --theme minimal --target ES6 --includeDeclarations --out dist/docs/ src/",
    "doc:deploy": "gh-pages -d ./dist/docs",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "prebuild": "rm -rf dist/*",
    "prepublish": "npm run build",
    "pretest": "npm run lint",
    "test": "jest --coverage"
  },
  "author": "Fletcher91 <thom@argu.co>",
  "license": "LGPL-3.0",
  "peerDependencies": {
    "es6-promise": ">= 4.x",
    "http-status-codes": ">= 1.x",
    "link-lib": ">=1.0.0-beta29",
    "prop-types": ">= 15.x",
    "rdflib": ">= 0.17.x",
    "react": ">= 16.x"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.13",
    "@types/jest": "^23.3.10",
    "@types/prop-types": "^15.5.8",
    "@types/react": "^16.7.17",
    "@types/react-redux": "^6.0.11",
    "enzyme": "3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "es6-promise": "4.x",
    "gh-pages": "^1.2.0",
    "http-status-codes": "1.x",
    "immutable": "3.8.x",
    "jest": "^23.5.0",
    "jest-enzyme": "^6.0.3",
    "link-lib": "=1.0.0-beta29",
    "prop-types": "^15.x",
    "rdflib": "0.17.x",
    "react": "=16.7.0-alpha.2",
    "react-dom": "=16.7.0-alpha.2",
    "react-redux": "^6.0.0",
    "react-test-renderer": "=16.7.0-alpha.2",
    "rollup": "^0.64.1",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.18.1",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "type-zoo": "^3.1.0",
    "typedoc": "^0.11.1",
    "typescript": "^3.2.2",
    "whatwg-fetch": "^2.0.4",
    "whatwg-url": "^6.5.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "enableTsDiagnostics": false
      }
    },
    "testMatch": [
      "**/*.spec.ts",
      "**/*.spec.tsx"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "setupTestFrameworkScriptFile": "./test/setup.js",
    "testURL": "http://example.org/resources/5",
    "transform": {
      "\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
}
